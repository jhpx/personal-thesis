﻿@Article{Sharma2024,
author={Sharma, Mayank
and Maity, Tanmoy},
title={Review on Machine Learning-Based Underground Coal Mines Gas Hazard Identification and Estimation Techniques},
journal={Archives of Computational Methods in Engineering},
year={2024},
month={Jan},
day={01},
volume={31},
number={1},
pages={371-388},
abstract={The underground coal mines (UCM) exhibit many life-threatening hazards for mining workers. In contrast, gas hazards are among the most critical challenges to handle. This study presents a comparative study of the sensor fusion methodologies related to UCM gas hazard prediction and classification. The study provides a brief theoretical background of the existing methodologies and their usage to mitigate the gas hazard issues in UCM. A brief comparison report emphasising the advantages and disadvantages of the existing models related to the UCM gas hazard monitoring is presented. Additionally, a separate comparison is also drawn, considering only neural network models based on their prediction accuracy and other performance metrics. This study attempts to observe and compare the Neural network models with the conventional method in the field of UCM gas hazard prediction, which is not explored in this fraternity.},
issn={1886-1784},
doi={10.1007/s11831-023-09982-1},
url={https://doi.org/10.1007/s11831-023-09982-1}
}

